<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="SCM Credentials" max_length="40" name="x_snc_ippi_scm_credentials" type="collection">
        <element label="Authtoken" max_length="255" name="authtoken" type="password2"/>
        <element display="true" label="Credential Name" max_length="40" name="credential_name" type="string"/>
        <element label="Password" max_length="255" name="password" type="password2"/>
        <element choice="3" default="Basic Auth" label="Type" max_length="4000" name="type" type="choice">
            <choice>
                <element label="Auth Token" value="Token"/>
                <element label="Basic Auth" value="Basic"/>
            </choice>
        </element>
        <element label="User" max_length="48" name="user" type="string"/>
    </element>
</database>
